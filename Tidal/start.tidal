d1 $ silence

d2 $ sound "CL_KICKS:24/2 CL_KICKS:26*2" # amp 0.4

d2 $ silence


d3 $ n "[0 6@0.25 1 2@0.5 0.25 3 5@0.25, 8!8]" # sound "H_DR:29" # speed 3 # amp 0.15

hush

